calibrate(alpha , beta, POIWall, POIWall, POIWall)
{
	PoI interests = scan();
	if(interests.contains(wallmark1) && interests.contains(wallmark2) && interests.contains(wallmark3){ 
		
	getAlphaBeta(3 x POIWall)
	general(alpha , beta, POIWall, POIWall, POIWall)
	}else{
	// ryk sig til en anden position og scan
	}
	
	return Vector3D // med dronens koordinator
}

List<POI> scan()
{

	POIsFound = List<POI>
	
	for (i = 0 til 360)
	{
		POIsFound.Add ( compareImages( ? ) )
	}
	
	return List<POI> // Returnere til global liste af POIs

}


